XLSceneCollectionPopupWidget {
    background: @defaultWidgetBgColor@;
}
XLSceneCollectionPopupWidget>QPushButton {
    border: none;
    background: transparent;
    font-size: @mediumFontSize@;
    color: #535353;
}
