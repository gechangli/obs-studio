QDialog#XLAddTextDialog {
    background: #f8f8f8;
    border: 1px solid #64c112;
}
QWidget#preview {
    border: 1px solid #d2d2d2;
    background: #e7e7e7;
}
QLabel#textLabel,QLabel#fontLabel,QLabel#colorLabel,QLabel#fontNameLabel {
    font-size: @bigFontSize@;
    color: black;
}
QTextEdit#textEdit {
    background-color: white;
    border: 1px solid #d2d2d2;
    border-radius: 4px;
}
QPushButton#selectFontButton {
    font-size: @bigFontSize@;
    border: none;
    border-radius: 8px;
    background-color: #64C123;
    color: white;
    padding-left: 4px;
    padding-right: 4px;
}
QPushButton#selectFontButton:hover {
    background-color: #64CD23;
}
QPushButton#selectFontButton:pressed {
    background-color: #64C123;
}
QPushButton#customizeButton {
    border: none;
    font-size: @bigFontSize@;
    color: #535353;
    background-color: transparent;
}
QPushButton#customizeButton:hover {
    color: #757575;
}
QPushButton#customizeButton:pressed {
    color: #535353;
}
QPushButton#yesButton {
    font-size: @bigFontSize@;
    border: none;
    border-radius: 16px;
    background-color: #64C123;
    color: white;
}
QPushButton#yesButton:hover {
    background-color: #64CD23;
}
QPushButton#yesButton:pressed {
    background-color: #64C123;
}
QPushButton#noButton {
     background-color: rgb(248,248,248);
     font-size: @bigFontSize@;
     color: rgb(203,202,202);
     border: 1px solid rgb(203,202,202);
     border-radius: 16px;
}
QPushButton#noButton:hover {
     background-color: rgb(203,202,202);
     color: rgb(248,248,248);
}
QPushButton#noButton:pressed {
     background-color: rgb(248,248,248);
     color: rgb(203,202,202);
}
